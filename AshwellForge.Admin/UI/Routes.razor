@using AshwellForge.Admin.UI.Pages

<Router AppAssembly="@AssemblyReference.Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <PageTitle>Ashwell Forge - Not Found</PageTitle>
        <LayoutView Layout="@typeof(MainLayout)">
            <PageNotFound />
        </LayoutView>
    </NotFound>
</Router>
