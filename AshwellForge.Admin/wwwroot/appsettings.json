{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AshwellForgeSettings": {
    "BaseAddress": "/",
    "StreamsBaseAddress": "/api/v1/streams",
    "HasHttpFlvPreview": true,
    "HttpFlvUriPattern": "/{streamPath}.flv",
    "HasHlsPreview": true,
    "HlsUriPattern": "/{streamPath}/output.m3u8",
    "AccessToken": "<access_token>"
  }
}
